pipeline{

    agent any

    stages{

        stage('sonar quality status'){

            agent{

                docker{
                    image 'maven:3.0.2-alpine'
                    args '-u root'
                }
            }
            steps{

                script{

                    withSonarQubeEnv(credentialsId: 'sonar-token') {
                   
                    sh 'mvn clean package sonar:sonar -X'

                 }

                }
            }
        }
    }
}